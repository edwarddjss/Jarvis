{
  "name": "jarvis",
  "version": "1.0.0",
  "description": "Simple demo of ElevenLabs Conversational AI WebSocket that enables Discord uses to talk to LLM's via voice.",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "start": "npm run build && node dist/index.js",
    "dev": "nodemon --exec ts-node src/index.ts",
    "postinstall": "tsc",
    "format": "prettier --check .",
    "format:fix": "prettier --write .",
    "lint": "eslint . --cache",
    "lint:fix": "eslint . --fix --cache"
  },
  "dependencies": {
    "@discordjs/opus": "^0.9.0",
    "@discordjs/voice": "^0.18.0",
    "@types/node": "^20.10.0",
    "bufferutil": "^4.0.8",
    "discord.js": "^14.14.1",
    "dotenv": "^16.3.1",
    "elevenlabs": "^0.18.1",
    "fluent-ffmpeg": "^2.1.3",
    "pino": "^9.0.0",
    "pino-pretty": "^13.0.0",
    "sodium-native": "^4.1.1",
    "ws": "^8.18.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.11.1",
    "@types/fluent-ffmpeg": "^2.1.27",
    "@types/pino": "^7.0.4",
    "eslint": "^9.11.1",
    "globals": "^15.9.0",
    "nodemon": "^3.0.2",
    "prettier": "^3.3.3",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.2",
    "typescript-eslint": "^8.8.0"
  },
  "engines": {
    "node": ">=16.x"
  }
}
